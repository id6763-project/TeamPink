{
  "name": "teampink",
  "private": true,
  "scripts": {
    "start": "run-p start:mb1",
    "app": "run-p start:api start:ui",
    "start:mb1": "nodemon packages/major-beat1/lib/index.js",
    "start:api": "cd packages/api && yarn start",
    "start:ui": "cd packages/ui && yarn start",
    "clean": "run-p clean:im clean:mb1 clean:api",
    "clean:im": "rimraf ./packages/interaction-molecule/lib ./packages/interaction-molecule/tsconfig.tsbuildinfo",
    "clean:mb1": "rimraf ./packages/major-beat1/lib ./packages/major-beat1/tsconfig.tsbuildinfo",
    "clean:api": "rimraf ./packages/api/dist ./packages/api/tsconfig.tsbuildinfo",
    "build": "run-s build:im build:mb1",
    "build:im": "tsc -b ./packages/interaction-molecule/tsconfig.json",
    "build:mb1": "tsc -b ./packages/major-beat1/tsconfig.json",
    "dev:im": "tsc -b -w ./packages/interaction-molecule/tsconfig.json",
    "dev:mb1": "tsc -b -w ./packages/major-beat1/tsconfig.json",
    "dev:api": "cd packages/api && yarn start:dev",
    "dev:ui": "cd packages/ui && yarn dev",
    "dev": "run-p dev:im dev:mb1",
    "serve:audio": "cd packages/audio-files && yarn start",
    "bootstrap": "lerna bootstrap",
    "postinstall": "run-s bootstrap clean",
    "deploy:mb1": "node packages/major-beat1/lib/index.js"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.6.0",
    "@nestjs/schematics": "^7.3.0",
    "@nestjs/testing": "^7.6.15",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.36",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "lerna": "^4.0.0",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.4",
    "ts-loader": "^8.0.18",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "@nestjs/common": "^7.6.15",
    "@nestjs/config": "^2.0.0",
    "@nestjs/core": "^7.6.15",
    "@nestjs/platform-express": "^7.6.15",
    "johnny-five": "^2.1.0",
    "node-pixel": "^0.11.0",
    "play-audio": "^0.5.2",
    "play-sound": "^1.1.5",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.6",
    "serialport": "^10.4.0"
  },
  "workspaces": [
    "packages/interaction-molecule",
    "packages/major-beat1"
  ]
}
