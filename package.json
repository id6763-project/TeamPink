{
  "name": "teampink",
  "private": true,
  "scripts": {
    "start": "run-p start:mb1",
    "start:mb1": "nodemon packages/major-beat1/lib/index.js",
    "clean": "run-p clean:im clean:mb1",
    "clean:im": "rimraf ./packages/interaction-molecule/lib ./packages/interaction-molecule/tsconfig.tsbuildinfo",
    "clean:mb1": "rimraf ./packages/major-beat1/lib ./packages/major-beat1/tsconfig.tsbuildinfo",
    "build": "run-s build:im build:mb1",
    "build:im": "tsc -b ./packages/interaction-molecule/tsconfig.json",
    "build:mb1": "tsc -b ./packages/major-beat1/tsconfig.json",
    "dev:im": "tsc -b -w ./packages/interaction-molecule/tsconfig.json",
    "dev:mb1": "tsc -b -w ./packages/major-beat1/tsconfig.json",
    "dev": "run-p dev:im dev:mb1",
    "bootstrap": "lerna bootstrap",
    "postinstall": "run-s bootstrap clean build",
    "deploy:mb1": "node packages/major-beat1/lib/index.js"
  },
  "devDependencies": {
    "lerna": "^4.0.0",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "typescript": "^4.6.3"
  },
  "workspaces": [
    "packages/interaction-molecule",
    "packages/major-beat1"
  ],
  "dependencies": {
    "johnny-five": "^2.1.0",
    "tone": "^14.7.77"
  }
}
